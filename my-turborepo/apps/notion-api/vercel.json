{
    "version": 2,
    "builds": [
      {
        "src": "next.config.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/api/$1" },
      { "src": "/(.*)", "dest": "/$1" }
    ],
    "crons": [
      {
        "path": "/api/users/run_users_sync_job",
        "schedule": "*/10 * * * *"
      }
    ]
  }